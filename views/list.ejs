

<%- include('header')   -%>
    <div class="box" id="heading">
        <h1 style="color:#fff">
            <%=listTitle %> 
        </h1>
    </div>
    <div class="box">


        <% for(var i=0;i<newListItems.length;i++){ %>
            <div class="item">

                <input type="checkbox">
                <p>
                    <%=newListItems[i]%>
                </p>
            </div>
            <%} %>


                <form action="/" method="post">
                    
                    <input type="text" name="newItem"  autocomplete="off" placeholder="Add a Task">
                    <button type="submit" name="list" value=<%=listTitle%>>+</button>


                </form>
    </div>

<%- include('footer') -%>






